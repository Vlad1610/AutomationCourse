<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="Print">
    <listeners>
        <listener class-name="org.testng.reporters.EmailableReporter"></listener>
        <listener class-name="org.testng.reporters.EmailableReporter2"></listener>
    </listeners>
    <test thread-count="5" name="TestAutomationProject">
        <groups>
            <run>
                <include name="print"/>
            </run>
        </groups>
        <classes>
            <class name="OldTests.TestActivity"/>
        </classes>
    </test> <!-- TestAutomationProject -->
</suite> <!-- Default Suite -->
